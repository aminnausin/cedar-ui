{
  "name": "@aminnausin/cedar-ui",
  "version": "0.0.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aminnausin/cedar-ui.git",
    "directory": "packages/cedar-ui"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "vue",
    "ui",
    "components",
    "shadcn",
    "cedar-ui",
    "tailwind",
    "typescript",
    "pines-ui"
  ],
  "main": "./dist/cedar-ui.umd.mjs",
  "module": "./dist/cedar-ui.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/cedar-ui.mjs",
      "require": "./dist/cedar-ui.umd.mjs"
    }
  },
  "files": [
    "./dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && npm run build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish  --no-git-checks --access public"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "fast-deep-equal": "^3.1.3",
    "focus-trap": "^7.6.5",
    "tailwind-merge": "^3.3.1",
    "tailwind-scrollbar-hide": "^2.0.0"
  },
  "peerDependencies": {
    "@tailwindcss/aspect-ratio": ">=0.4.2",
    "@tailwindcss/forms": ">=0.5.10",
    "@vueuse/components": ">=10.11.1 <14.0.0",
    "@vueuse/core": ">=10.11.1 <14.0.0",
    "@vueuse/integrations": ">=11.3.0 <14.0.0",
    "tailwindcss": ">=3.4.17",
    "vue": ">=3.5.13 <4.0.0",
    "vue-router": ">=4.5.0 <5.0.0"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.17.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "typescript": "~5.9.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.12"
  }
}
